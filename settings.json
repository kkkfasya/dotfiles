{
  "terminal.integrated.tabs.location": "left",
  "explorer.confirmDelete": false,
  "files.autoSave": "onFocusChange",
  "editor.fontLigatures": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.centerLayout": false,
  "vsicons.dontShowNewVersionMessage": false,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "editor.fontWeight": "normal",
  "editor.minimap.autohide": true,
  "explorer.confirmDragAndDrop": false,
  "editor.unicodeHighlight.includeComments": true,
  "workbench.colorCustomizations": {
    "editorLineNumber.foeground": "#f3e2e2e3",
    "[Noctis Minimus]": {
      "statusBar.background": "#046fc7",
      "statusBar.foreground": "#ece8e8",
      "editor.selectionBackground": "#ff84d634",
      // "editor.selectionHighlightBorder": "#ffffffc2",
     "editor.selectionHighlightBackground": "#ffffff2d"
    },
    "[Gruvbox Dark Hard]": {
      "editorSuggestWidget.background": "#221a05",
      "editorSuggestWidget.selectedBackground": "#ebb4332c",
      "editorLineNumber.foreground": "#C6C6C6"
    },
  },
  "editor.tokenColorCustomizations": {
    "[Noctis Minimus]": {
      "comments": "#4db2ec",
      // "comments": "#d6d6d6e5",
      "numbers": "#5096ff",
      "keywords": {
        "fontStyle": "italic"
      }
    }
  },
  "editor.semanticTokenColorCustomizations": {
    "[Noctis Minimus]": {
      "enabled": true,
      "rules": {
        "class": "#e5c07b",
        "parameter": {
          "foreground": "#f09383d5",
          "fontStyle": "italic"
        },
        "*.decorator": "#46a1ecd7",
      }
    },
    "[Gruvbox Dark Hard]": {
      "enabled": true,
      "rules":{
        "parameter": {
          "foreground": "#d65e0e",
          "fontStyle": "italic",
        },
        "variable": "#fbf1c7da"
      }
    },
},


  "explorer.sortOrder": "type",
  "terminal.integrated.gpuAcceleration": "auto",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.typeCheckingMode": "off",
  "vsicons.presets.jsonOfficial": true,
  "editor.linkedEditing": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "workbench.layoutControl.enabled": false,
  "glassit.alpha": 240,
  "liveServer.settings.donotVerifyTags": true,
  "css.completion.completePropertyWithSemicolon": false,
  "terminal.integrated.cursorBlinking": true,
  "emmet.excludeLanguages": [
    "markdown",
    "css"
  ],
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnType": true
  },
  "python.formatting.provider": "black",
  "errorLens.enabledDiagnosticLevels": [
    "warning",
    "info",
    "hint",
    "error"
  ],
  "comments.useRelativeTime": false,
  "http.proxyAuthorization": null,
  "terminal.integrated.fontSize": 12,
  "gruvboxMaterial.darkCursor": "orange",
  "gruvboxMaterial.darkSelection": "orange",
  "gruvboxMaterial.italicKeywords": true,
  "gruvboxMaterial.darkContrast": "hard",
  "files.insertFinalNewline": true,
  "gruvboxMaterial.darkPalette": "original",
  "gruvboxMaterial.darkWorkbench": "flat",
  "editor.minimap.enabled": false,
  "vim.smartRelativeLine": true,
  "editor.cursorSurroundingLines": 7,
  "vim.useSystemClipboard": true,
  "keyboard.dispatch": "keyCode",
  "vim.highlightedyank.enable": true,
  "editor.bracketPairColorization.enabled": false,
  "editor.guides.bracketPairs": "active",
  "code-runner.saveFileBeforeRun": true,
  "code-runner.enableAppInsights": false,
  "code-runner.clearPreviousOutput": true,
  "telemetry.telemetryLevel": "off",
  "window.menuBarVisibility": "toggle",
  "editor.cursorBlinking": "solid",
  "vim.cursorStylePerMode.insert": "line",
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on"
  },
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
  {
    "before": ["<leader>","e"],
    "commands": ["workbench.view.explorer"]
  },
  {
    "before": ["<leader>","p"],
    "commands": ["workbench.action.quickOpen"]
  },
    {
      "before": ["g", "p"],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["g", "h"],
      "commands": ["editor.action.showDefinitionPreviewHover"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "p", "i"],
      "commands": ["editor.action.peekImplementation"]
    },
    {
      "before": ["g", "q"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.referenceSearch.trigger"]
    },
    {
      "before": ["g", "t"],
      "commands": ["editor.action.goToTypeDefinition"]
    },
    {
      "before": ["g", "p", "t"],
      "commands": ["editor.action.peekTypeDefinition"]
    },
  ],
  "vim.highlightedyank.textColor":"#f09383d5",
  "vim.highlightedyank.duration": 150,

  "code-runner.runInTerminal": true,
  "editor.smoothScrolling": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "quokka.compactMessageOutput": false,
  "quokka.showOutputOnStart": false,
  "prettier.tabWidth": 4,
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "editor.inlineSuggest.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
}
